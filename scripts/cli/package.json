{
  "name": "@polymeshassociation/polymesh-cli",
  "version": "1.0.0",
  "description": "CLI for Polymesh",
  "main": "/dist/polymesh_cli.js",
  "private": true,
  "bin": {
    "polymesh-cli": "./dist/polymesh_cli.js"
  },
  "scripts": {
    "start": "npm run build",
    "create": "npm run build && npm run test",
    "build": "npm run clean:some && tsc -p .",
    "build:types": "npm run update:types && npm run generate:polymesh-meta && npm run generate:defs && npm run generate:meta",
    "update:types": "node ./util/updateTypes.js",
    "generate:defs": "node --loader @polkadot/dev-ts node_modules/.bin/polkadot-types-from-defs --package polymesh-types --endpoint ./polymesh-meta.json --input ./src/interfaces",
    "generate:meta": "node --loader @polkadot/dev-ts node_modules/.bin/polkadot-types-from-chain --package polymesh-types --endpoint ./polymesh-meta.json --output ./src/interfaces",
    "generate:polymesh-meta": "./chain_metadata.sh",
    "lint": "tsc --noEmit --pretty",
    "clean:some": "rm -rf ./dist ./docs",
    "clean:all": "rm -rf ./node_modules package-lock.json ./dist ./docs",
    "tscov": "type-coverage --at-least 95",
    "tscov:d": "type-coverage --detail",
    "refresh": "npm run clean:all && npm install",
    "publish-package": "npm run build && npm run publish",
    "docs": "typedoc src/tests src/helpers src/util --out dist/docs ",
    "00_create_identities": "jest --runInBand 00_create_identities",
    "01_poly_transfer": "jest --runInBand 01_poly_transfer",
    "02_key_management": "jest --runInBand 02_key_management",
    "03_auth_join_did": "jest --runInBand 03_auth_join_did",
    "04_permission_management": "jest --runInBand 04_permission_management",
    "05_claim_management": "jest --runInBand 05_claim_management",
    "06_create_assets": "jest --runInBand 06_create_assets",
    "07_create_claim_compliance": "jest --runInBand 07_create_claim_compliance",
    "08_asset_minting": "jest --runInBand 08_asset_minting",
    "10_governance_management": "jest --runInBand 10_governance_management",
    "11_A_settlement": "jest --runInBand 11_A_settlement",
    "11_B_settlement": "jest --runInBand 11_B_settlement",
    "12_external_agents": "jest --runInBand 12_external_agents",
    "13_corporate_actions": "jest --runInBand 13_corporate_actions",
    "14_investment": "jest --runInBand 14_investment",
    "15_portfolio": "jest --runInBand 15_portfolio",
    "16_relayer": "jest --runInBand 16_relayer",
    "schema_test": "./util/schema_test.sh",
    "offchain_worker_test": "node ./dist/util/offchain_worker_test.js",
    "polyx_balances": "node ./util/userBalancePOLYX.js",
    "test": "jest --runInBand --silent && ./util/schema_test.sh",
    "clear_cache": "jest --clearCache"
  },
  "author": "Polymesh Association",
  "license": "UNLICENSED",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@polkadot/api": "11.2.1",
    "@polkadot/util": "12.6.2",
    "@polkadot/util-crypto": "12.6.2",
    "chai": "4.2.0",
    "chalk": "4.1.0",
    "clear": "0.1.0",
    "cli-progress": "3.10.0",
    "command-line-args": "5.2.0",
    "commander": "8.3.0",
    "crypto-random-string": "4.0.0",
    "esm": "3.2.25",
    "export-to-csv": "^0.2.1",
    "figlet": "1.5.2",
    "fp-ts": "2.11.7",
    "inquirer": "8.2.0",
    "minimist": "1.2.6",
    "nodejs-fs-utils": "1.2.6",
    "path": "0.12.7",
    "pm2": "^5.3.1",
    "readline-sync": "1.4.10",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-transform-modules-commonjs": "7.22.11",
    "@polkadot/dev-ts": "^0.79.3",
    "@polkadot/typegen": "11.2.1",
    "@types/chai": "4.2.15",
    "@types/clear": "0.1.1",
    "@types/figlet": "1.2.1",
    "@types/inquirer": "7.3.1",
    "@types/jest": "^29.5.12",
    "@types/node": "14.14.22",
    "babel-polyfill": "^6.26.0",
    "jest": "29.6.4",
    "nodemon": "^3.0.1",
    "pretty-error": "^3.0.4",
    "ts-jest": "^29.1.5",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "type-coverage": "2.16.3",
    "typescript": "4.6.2"
  }
}
